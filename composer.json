{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "abraham/twitteroauth": "^0.7.4",
        "algolia/algoliasearch-client-php": "^1.23",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.0",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "spatie/laravel-backup": "^5.1",
        "spatie/laravel-csp": "^1.0",
        "spatie/laravel-feed": "^2.0",
        "spatie/laravel-menu": "^3.0",
        "spatie/laravel-missing-page-redirector": "^2.1",
        "spatie/laravel-responsecache": "^4.1",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.0",
        "spatie/laravel-tail": "^3.0",
        "stripe/stripe-php": "^6.8",
        "themsaid/laravel-mail-preview": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "friendsofphp/php-cs-fixer": "^2.8",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "sensiolabs/security-checker": "^4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
