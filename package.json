        "laravel-mix-purgecss": "^1.0",
        "lodash": "^4.17.4",
        "tailwindcss": "^0.4",
        "vue": "^2.5.13"
    },
    "dependencies": {
        "algoliasearch": "^3.24.5",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "glob-all": "^3.1.0",
        "highlight.js": "^9.12.0",
        "simplemde": "^1.11.2",
        "turbolinks": "^5.0.3",
        "vue-turbolinks": "^2.0.0"
